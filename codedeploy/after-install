mv willy.jar
cat > /etc/systemd/system/willy.service <<EOF
[Unit]
Description=Willy service
After=network.target
StartLimitIntervalSec=0
[Service]
Type=simple
Restart=always
RestartSec=1
User=willy
ExecStart=java --add-opens java.base/java.lang=ALL-UNNAMED -jar ./willy.jar

[Install]
WantedBy=multi-user.target
EOF